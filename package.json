{
  "name": "ai-agent-server",
  "version": "1.0.0",
  "description": "Pluggable AI Agent Server with RAG and Plugin System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
